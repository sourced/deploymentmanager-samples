# Example of the Cloud Router template usage.
#
# Replace `test-network` with your network's name.
{% set environment = 'prod' %}
name: my-cloudrouter-{{environment}}
description: My cloud router deployment for {{environment}} environment
project: mytestproject-vpun-test1

imports:
  - path: templates/cloud_router/cloud_router.py
    name: cloud_router.py

resources:
  - name: my-cloudrouter-{{environment}}
    type: cloud_router.py
    properties:
      name: cloud-router
      network: !DMOutput dm://mytestproject-vpun-test1/my-network-{{environment}}/my-network-prod/name
      region: us-east1
      asn: 65001
